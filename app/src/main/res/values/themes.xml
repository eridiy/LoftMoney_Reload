<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LoftMoney_Reload" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/PantoneCoolGray2C0</item>
        <item name="colorPrimaryVariant">@color/Pantone5395C87</item>
        <item name="colorOnPrimary">@color/PantoneCoolGray2C20</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/PantoneCoolGray2C0</item>
        <item name="colorSecondaryVariant">@color/Pantone1955C</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.LoftMoney_Reload.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.LoftMoney_Reload.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.LoftMoney_Reload.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>